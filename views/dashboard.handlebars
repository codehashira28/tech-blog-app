{{#each posts}}
<a class="w-100" href="/dashboard/post/{{id}}">
<div class="display-flex h-10 justify-space-between bg-info text-light p-2 post w-100" style="border-radius: 0.3rem; height: 3rem;">
    <div>{{title}}</div>
    <div>{{format_date createdAt}}</div>
</div>
</a>
{{/each}}

<a href="/dashboard/create" style="display: block"><button class="btn btn-primary" id="new-post">+ New Post</button></a>